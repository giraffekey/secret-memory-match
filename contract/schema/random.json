{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Random",
  "type": "object",
  "required": [
    "counter",
    "seed"
  ],
  "properties": {
    "counter": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "seed": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      },
      "maxItems": 32,
      "minItems": 32
    }
  }
}
